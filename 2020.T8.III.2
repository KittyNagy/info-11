#include <iostream>

using namespace std;

int main()
{
    int n, k;
    cout<<"n=";
    cin>>n;
    cout<<"k=";
    cin>>k;
    int v[n][n];
    for(int i=0; i<n; i++){
        for(int j=0; j<n; j++){
                cout<<"v["<<i<<"]["<<j<<"]=";
                cin>>v[i][j];
    }}
    int atm=v[k-1][0];
    for(int i=0; i<k-2; i++){
        v[k-1][i]=v[k-1][i+1];
    }
    v[k-1][k-2]=atm;
    for(int i=0; i<n; i++){
        for(int j=0; j<n; j++){
                cout<<v[i][j]<<" ";
    }
    cout<<endl;}
    return 0;
}
