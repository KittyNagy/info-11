#include <iostream>
#include <fstream>

using namespace std;

int main()
{
    int sz[10]={0}, maxi=-1,a, poz[10]={0}, counter=0;
    ifstream in("bac.in");
    while (in>>a){
        sz[a%10]++;
        counter++;
        if(sz[a%10]>maxi){maxi=sz[a%10];}
        poz[a%10]=counter;
    }
    for(int i=0; i<10; i++){
        if(sz[i]==maxi){cout<<poz[i]<< " ";}
    }
    return 0;
}
